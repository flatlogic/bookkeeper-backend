{
  "name": "cp_for_web",
  "version": "1.0.0",
  "description": "CP for Web",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run dev:start",
    "dev:start": "npm-run-all build start",
    "migrations:create": "typeorm migration:generate -n Migration && git add src/migrations/* || true",
    "migrations:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "heroku-postbuild": "npm install --production",
    "postinstall": "npm run build"
  },
  "author": "Alexander Dolbik",
  "license": "ISC",
  "engines": {
    "node": "10.x"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "change-case": "^3.1.0",
    "class-validator": "^0.10.0",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-validator": "^6.2.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.12.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.18",

    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/express-validator": "^3.0.0",
    "@types/joi": "^14.3.3",
    "@types/jsonwebtoken": "^8.3.4",
    "@types/lodash": "^4.14.144",
    "@types/node": "^12.7.5",
    "@types/passport": "^1.0.1",
    "@types/passport-jwt": "^3.0.2",
    "typescript": "^3.6.3"
  },
  "devDependencies": {
    "nodemon": "^1.19.2",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.3.0",
    "tslint": "^5.20.0"
  }
}
